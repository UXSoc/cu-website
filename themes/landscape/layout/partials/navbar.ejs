<div class="background">
    <div class="container pt-5 pb-4">
        <div class="row align-items-center">
            <div class="col-auto">
                <p class="font-weight-bold h1"><%= config.title %></p>
            </div>
            <div class="col-1"></div>

            <% for (var i = 0; i < config.navbar.entries.length; i++) { %>
                <% currenEntry = config.navbar.entries[i] %>
                <div class="col-auto">
                    <a href="<%= currenEntry[1] %>" class="navbar-link">
                        <p class="h2 "><%= currenEntry[0] %></p>
                    </a>
                </div>
            <% } %>

        </div>
    </div>
</div>