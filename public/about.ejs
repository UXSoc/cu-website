<!DOCTYPE html>
<html>
    <head>
        <%- partial("partials/header.ejs") %>
    </head>

    <body>

      <%- partial("partials/navbar.ejs") %>

      <div class="jumbotron jumbotron-fluid">
        <div class="container">
          <h1 class="display-4">About Club</h1>
          <p class="lead">UX Society is a student-run organization that advocates and practices human-centered design in solving problems in the community.
          </p>
        </div>
      </div>

      <div class="container">
        <h1 class="display-4 mb-3">Members</h1>

        <div class="row">
        <% for (var article in public._data) { %>
            <div class="col-sm mb-5">
              <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="img/<%= public._data[article].picture %>" alt="Card image cap">
                <div class="card-body">
                  <h5 class="card-title"><%= public._data[article].name %></h5>
                  <p class="card-text">Major: <%= public._data[article].major %></p>
                  <p class="card-text">Role: <%= public._data[article].role %></p>
                </div>
              </div>
            </div>
        <% } %>
        </div>
      </div>




      <%- partial("partials/footer.ejs") %>

        <div id="scripts">
            <!-- external CDN scripts  -->
            <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

            <!-- Local JS -->
            <script src="javascript/about.js.js"></script>
        </div>
    </body>

</html>
